{
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/api/src",
  "monorepo": true,
  "root": "apps/api",
  "compilerOptions": {
    "webpack": true,
    "tsConfigPath": "apps/api/tsconfig.ap.json"
  },
  "projects": {
    "api": {
      "type": "application",
      "root": "apps/api",
      "entryFile": "main",
      "sourceRoot": "apps/api/src",
      "compilerOptions": {
        "tsConfigPath": "apps/api/tsconfig.app.json"
      }
    },
    "admin": {
      "type": "application",
      "root": "apps/admin",
      "entryFile": "main",
      "sourceRoot": "apps/admin/src",
      "compilerOptions": {
        "tsConfigPath": "apps/admin/tsconfig.app.json"
      }
    },
    "entity": {
      "type": "library",
      "root": "libs/entity",
      "entryFile": "index",
      "sourceRoot": "libs/entity/src",
      "compilerOptions": {
        "tsConfigPath": "libs/entity/tsconfig.lib.json"
      }
    },
    "board": {
      "type": "application",
      "root": "apps/board",
      "entryFile": "main",
      "sourceRoot": "apps/board/src",
      "compilerOptions": {
        "tsConfigPath": "apps/board/tsconfig.app.json"
      }
    },
    "social-media-scheduler": {
      "type": "application",
      "root": "apps/social-media-scheduler",
      "entryFile": "main",
      "sourceRoot": "apps/social-media-scheduler/src",
      "compilerOptions": {
        "tsConfigPath": "apps/social-media-scheduler/tsconfig.app.json"
      }
    }
  }
}